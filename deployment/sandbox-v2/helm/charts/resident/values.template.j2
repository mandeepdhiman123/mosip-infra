component: resident
part-of: mosip
activeProfileEnv: mz 
springConfigLabel: {{ config_repo.branch }} 
configServerUrl: {{ config_server.internal_url }}
containerUserId: {{ container_user_id }}
containerUserGid: {{ container_user_gid }}
containerWorkDir: {{ container_work_dir }}
podUserId: {{ pod_user_id }}
podUserGid: {{ pod_user_gid }}
service_account: {{ psp_service_account }}
ingress:
   namespace: default
artifactoryUrl: {{ artifactory_url }} 
imagePullPolicy: Always

isGlowroot: {{ is_glowroot }} 

probe:
  initialDelay: 30  # Seconds
  period: 30 # Seconds

dockerHub:
  private: {{ docker_hub.private }}  
  keyname: {{ docker_hub.keyname }}

services:
  resident: 
    serviceName: {{ versions.services.resident.resident.keys()[0] }} 
    dockerImage: {{ versions.services.resident.resident.values()[0] }}
    dockerTag: 1.0.10
    containerPort: 8099
    servicePort: 80
    ingressPath: /resident/v1
    healthUrl: /resident/v1/actuator/health
